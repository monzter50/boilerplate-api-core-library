{
  "name": "boilerplate-api-core-library",
  "version": "1.0.2",
  "description": "Only i want practices and understand how create first library for implement my other projects <3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/client/index.js",
      "import": "./dist/client/index.mjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "require": "./dist/server/index.js",
      "import": "./dist/server/index.mjs"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsup",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "version": "changeset version",
    "release": "yarn run build && changeset publish",
    "example:server": "tsx examples/server/server-example.ts",
    "example:client:build": "tsup examples/client/web-example.ts --format iife --out-dir examples/client/dist --no-splitting --global-name webExample --no-dts --clean",
    "example:client:serve": "http-server examples/client -o -c-1"
  },
  "author": "Monster Codes",
  "keywords": [
    "api",
    "fetch",
    "library",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monzter50/boilerplate-api-core-library.git"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^8.57.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.2.4",
    "tsx": "^4.19.4",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "overrides": {
    "string-width": "^4.2.3"
  },
  "resolutions": {
    "string-width": "^4.2.3"
  }
}
