{
  "name": "boilerplate-api-core-library",
  "version": "1.0.0",
  "description": "Only i want practices and understand how create first library for implement my other projects <3",
  "main": "dist/client/index.js",
  "module": "dist/client/index.mjs",
  "types": "dist/client/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    },
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/client/index.ts src/server/index.ts --format cjs,esm --dts",
    "test": "jest",
    "release": "yarn run build && changeset publish"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
